<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="duplicate_calendar_event_form_popup" model="ir.ui.view">
      <field name="name">duplicate_calendar_event_form_popup</field>
      <field name="model">calendar.event</field>
      <field name="inherit_id" ref="calendar.view_calendar_event_form_popup" />
      <field name="arch" type="xml">
        <xpath expr="//group" position="before">
          <button string="Duplicate" name="copy" type="object" class="oe_stat_button float-right" icon="fa-copy" />
        </xpath>
      </field>
    </record>
